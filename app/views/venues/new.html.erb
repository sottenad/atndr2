<h2>New Venue!</h2>

<%= form_for @venue do |f| %>
	<label>Name</label>
	<%= f.text_field :name %>
	<label>Raw Address</label>
	<%= f.text_field :raw_address %>
	<label>Address</label>
	<%= f.text_field :address %>
	<label>City</label>
	<%= f.text_field :city %>
	<label>Zip</label>
	<%= f.text_field :zip %>
	
	<%= f.submit "Create Venue" %>

<% end %>
